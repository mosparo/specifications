{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://schema.mosparo.io/rule-package-batch-ur.json",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "description": {
            "type": [
                "string",
                "null"
            ]
        },
        "type": {
            "type": "string"
        },
        "status": {
            "type": "boolean"
        },
        "spamRatingFactor": {
            "type": "number"
        },
        "uuid": {
            "type": "string"
        }
    },
    "required": [
        "name",
        "type",
        "uuid"
    ],
    "additionalProperties": false
}
